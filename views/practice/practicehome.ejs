<h1>practice home</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Primary Chord</th>
            <th>Secondary Chord</th>
        </tr>
    </thead>
    <tbody>
        <% chordadd.forEach(function(c) { %>
        <tr>
            <td><%= c.primaryChord %></td>
            <td><%= c.secondaryChord %></td>
        </tr>

        <% }) %>
</table>
</main>
