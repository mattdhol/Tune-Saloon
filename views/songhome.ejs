<%- include('partials/navbar') %>

<h1>Song Home</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Song Workbook</th>
            <th>Title</th>
            <th>Artist</th>
            <th>Appeal</th>
            <th>Difficulty</th>
            <th>Video Link</th>
            <th> Guitar Tab </th>
            <th> Delete Song</th>
        </tr>
    </thead>
    <tbody>
        <% allsongs.forEach(function(g) { %>
            <tr>
                <td> <a href="/<%=g._id%>"> Workbook</a> </td>
                <td><%= g.songTitle %></td>
                <td><%= g.songArtist %></td>
                <td><%= g.appeal %></td>
                <td><%= g.difficulty %></td>
                <td> <a href = "<%= g.videoLink%>"> Video </a> </td>
                <td> <a href = "<%= g.guitarTab%>"> Tab </a> </td>
                <td><a href="/delete/<%=g._id%>"> Delete</a> </td>
            </tr>   
        <% }) %>
    </tbody>
</table>

<%- include('partials/footer') %>
